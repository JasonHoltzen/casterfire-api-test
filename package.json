{
	"name": "casterfire-api-test",
	"version": "0.0.1",
	"scripts": {
		"build": "svelte-kit build",
		"dev": "svelte-kit dev",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"host": "svelte-kit dev -- --open --host",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"preview": "svelte-kit preview --host",
		"test": "NODE_OPTIONS=--experimental-vm-modules jest --verbose src --config jest.config.json",
		"test:stop": "NODE_OPTIONS=--experimental-vm-modules jest src --config jest.config.json",
		"test:stopWatch": "npm run test:stop -- --watch",
		"test:watch": "npm run test -- --watch"
	},
	"devDependencies": {
		"@babel/core": "^7.14.0",
		"@babel/preset-env": "^7.14.0",
		"@sveltejs/adapter-auto": "next",
		"@sveltejs/adapter-netlify": "^1.0.0-next.37",
		"@sveltejs/kit": "next",
		"@testing-library/svelte": "^3.0.0",
		"@types/testing-library__jest-dom": "^5.14.0",
		"autoprefixer": "^10.4.0",
		"babel-jest": "^27.0.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"jest": "^27.0.0",
		"prettier": "^2.4.1",
		"prettier-plugin-svelte": "^2.4.0",
		"sass": "^1.45.1",
		"svelte": "^3.44.0",
		"svelte-jester": "^2.0.1",
		"svelte-preprocess": "^4.10.1"
	},
	"type": "module",
	"dependencies": {
		"bcrypt": "^5.0.1",
		"cookie": "^0.4.1",
		"cssnano": "^5.0.14",
		"dotenv": "^10.0.0",
		"joi": "^17.5.0",
		"joigoose": "^8.0.2",
		"lodash.isequal": "^4.5.0",
		"marked": "^4.0.8",
		"mongoose": "^6.1.4",
		"postcss-load-config": "^3.1.1",
		"string-hash": "^1.1.3",
		"tweetnacl": "^1.0.3",
		"tweetnacl-util": "^0.15.1",
		"uuid": "^8.3.2"
	}
}
